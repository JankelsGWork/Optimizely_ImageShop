<?xml version="1.0" encoding="utf-8"?>
<module authorizationPolicy="episerver:cmsedit" clientResourceRelativePath="$version$">
    <assemblies>
        <add assembly="Imageshop.Optimizely.Plugin" />
    </assemblies>
    <dojo>
        <paths>
            <add name="imageshop-optimizely-plugin" path="ClientResources" />
        </paths>
    </dojo>
    <clientModule initializer="imageshop-optimizely-plugin/ImageshopInitializer">
        <moduleDependencies>
            <add dependency="CMS" type="RunAfter" />
			<add dependency="Shell" type="RunAfter" />
        </moduleDependencies>
    </clientModule>
</module>